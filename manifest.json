{
	"manifest_version": 2,
	"name": "Inject JS code",
	"description": "",
	"version": "0.1",
	"icons": {"16": "icon16.png", "128": "icon128.png"},
	"permissions": [
		"tabs",
		"downloads",
		"activeTab",
		"storage", "*://*/*","<all_urls>","unlimitedStorage",
		"contextMenus"
	],
	"background": {
		 "scripts": ["background.js"],
		 "persistent": false,
		 "all_frames": true,
    	 "match_about_blank": true
	 },
	"browser_action": {
		"default_title": "Open Dictionary i18n tool",
		"default_icon": "icon128.png"
		}
}
